{
  "functions": {
    "api/supporters.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/supporters",
      "dest": "/api/supporters.js"
    },
    {
      "src": "/api/supporters/(.*)",
      "dest": "/api/supporters.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
